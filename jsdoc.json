{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["scripts"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./out/",
    "recurse": true
  }
}
